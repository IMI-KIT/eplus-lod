<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mapping xmlns="http://imi.kit.edu/rdfMapping">
    <namespaces>
        <ns>
            <prefix>defaultns</prefix>
            <uri>http://dareed.eu/resources/</uri>
        </ns>
        <ns>
            <prefix>eplus</prefix>
            <uri>http://dareed.eu/ontology/energyplus#</uri>
        </ns>
        <ns>
            <prefix>rdfs</prefix>
            <uri>http://www.w3.org/2000/01/rdf-schema#</uri>
        </ns>
        <ns>
            <prefix>xsd</prefix>
            <uri>http://www.w3.org/2001/XMLSchema#</uri>
        </ns>
        <ns>
            <prefix>ssn</prefix>
            <uri>http://purl.oclc.org/NET/ssnx/ssn#</uri>
        </ns>
        <ns>
            <prefix>qb</prefix>
            <uri>http://purl.org/linked-data/cube#</uri>
        </ns>
        <ns>
            <prefix>om</prefix>
            <uri>http://www.wurvoc.org/vocabularies/om-1.8/</uri>
        </ns>
        <ns>
            <prefix>interval</prefix>
            <uri>http://reference.data.gov.uk/def/intervals/</uri>
        </ns>
    </namespaces>
    <entities>
        <entity>
            <uri>:${simulationId}/sensors/${variableId}</uri>
            <name>Sensor</name>
            <properties>
                <dataProperty>
                    <uri>eplus:variableId</uri>
                    <value>${variableId}</value>
                </dataProperty>
            </properties>
        </entity>
        <entity>
            <uri>:${simulationId}/observations/${variableId}/${property}</uri>
            <name>Observation</name>
            <type>ssn:Observation</type>
            <properties>
                <objectProperty>
                    <uri>ssn:featureOfInterest</uri>
                    <object>${resourceURL}</object>
                </objectProperty>
                <objectProperty>
                    <uri>ssn:observedProperty</uri>
                    <object>${propertyURL}</object>
                </objectProperty>
                <objectProperty>
                    <uri>ssn:observationResult</uri>
                    <object>${observationResultURL}</object>
                </objectProperty>
                <objectProperty>
                    <uri>ssn:observedBy</uri>
                    <object>${sensorURL}</object>
                </objectProperty>
            </properties>
        </entity>
        <entity>
            <uri>:${simulationId}/observation/${variableId}/${property}/result</uri>
            <name>ObservationResult</name>
            <type>eplus:OutputDataset</type>
            <type>ssn:ObservationResult</type>
        </entity>
        <entity>
            <uri></uri>
            <name>ObservationValue</name>
            <type>qb:Observation</type>
            <type>ssn:ObservationValue</type>
            <type>eplus:SimulationValue</type>
            <type>om:Measure</type>
            <properties>
                <objectProperty>
                    <uri>qb:dataSet</uri>
                    <object>${observationResultURL}</object>
                </objectProperty>
                <dataProperty>
                    <uri>om:numerical_value</uri>
                    <value>#{1}</value>
                </dataProperty>
                <objectProperty>
                    <uri>om:unit_of_measure_or_measurement_scale</uri>
                    <object>${unitURL}</object>
                </objectProperty>
                <dataProperty>
                    <uri>eplus:dayOfSimulation</uri>
                    <value>${dayOfSimulation}</value>
                </dataProperty>
                <dataProperty>
                    <uri>interval:ordinalMonthOfYear</uri>
                    <value>${month}</value>
                </dataProperty>
                <dataProperty>
                    <uri>interval:ordinalHourOfDay</uri>
                    <value>${hour}</value>
                </dataProperty>
                <dataProperty>
                    <uri>interval:ordinalDayOfMonth</uri>
                    <value>${day}</value>
                </dataProperty>
                <dataProperty>
                    <uri>eplus:startMinute</uri>
                    <value>${startMinute}</value>
                </dataProperty>
                <dataProperty>
                    <uri>eplus:endMinute</uri>
                    <value>${endMinute}</value>
                </dataProperty>
            </properties>
        </entity>
    </entities>
</mapping>